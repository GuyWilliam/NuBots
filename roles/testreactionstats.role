  platform::darwin::HardwareSimulator
  input::SensorFilter
  # Motion
  motion::QuinticWalk
  motion::HeadController
  # Behaviour
  behaviour::Controller
  behaviour::skills::DirectWalkController
  behaviour::strategy::KeyboardWalk
)
